    "lint:staged": "lint-staged",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 100",
    "bundle-report": "webpack-bundle-analyzer --port 4200 build/chrome/js/stats.json",
    "bundle-report-firefox": "webpack-bundle-analyzer --port 4200 build/firefox/js/stats.json"
  },
