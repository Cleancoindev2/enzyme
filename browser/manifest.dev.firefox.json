{
  "version": "0.0.4",
  "applications": {
    "gecko": {
      "id": "info@blockxlabs.com",
      "strict_min_version": "48.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "name": "Enzyme",
  "manifest_version": 2,
  "description": "Enzyme is a wallet for polkadot",
  "browser_action": {
    "default_icon": "img/icon-32.png",
    "default_title": "Enzyme",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16-dev.png",
    "32": "img/icon-32-dev.png",
    "128": "img/icon-128-dev.png"
  },
  "web_accessible_resources": ["inject.html", "content-script.js"],
  "permissions": ["notifications", "storage", "https://*/*", "wss://*/*"],
  "content_security_policy": "script-src 'self' https://localhost:3000 'unsafe-eval'; img-src * data:; object-src 'self';"
}
